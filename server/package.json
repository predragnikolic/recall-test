    "better-auth": "0.7.3-beta.1",
    "dotenv": "16.4.5",
    "drizzle-orm": "0.36.0",
    "drizzle-zod": "0.5.1",
    "glob": "11.0.0",
    "hono": "4.6.9",
    "pg": "8.13.1",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@types/node": "20.11.17",
    "@types/pg": "8.11.10",
    "drizzle-kit": "0.27.1",
    "tsx": "4.19.2",
    "typescript": "5.6.2"
  }
